<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- CommonTitleBar -->
    <declare-styleable name="CommonTitleBar">

        <attr name="textLeft" format="string" />
        <attr name="title" format="string" />
        <attr name="textRight" format="string" />
        <attr name="leftRes" format="reference" />
        <attr name="rightRes" format="reference" />
        <attr name="showBack" format="boolean" />
        <attr name="leftTextSize" format="dimension" />
        <attr name="titleSize" format="dimension" />
        <attr name="rightTextSize" format="dimension" />
        <attr name="titleColor" format="color" />
        <attr name="leftTextColor" format="color" />
        <attr name="rightTextColor" format="color" />

    </declare-styleable>
</resources>